.caption-2::after {
content: '\201C' attr(data-description) '\201D';
top: 0;
height: 70%;
background: #7A96EA;


font-size: 10px;
text-align: center;
}
.caption-2::before {
content: attr(data-title);
top: 70%;
height: 30%;
background: #A0B4F0;
font-size: 18px;
font-weight: 300;
text-align: center;
}
.caption {
display: inline-block;
position: relative;

}

.caption-2:hover::before, .caption-2:hover::after {
opacity: 1;
}
.caption-2::before {
opacity: 0;
position: absolute;
width: 100%;
color: #fff;
padding: 20px;
-webkit-transition: opacity 0.3s;
-moz-transition: opacity 0.3s;
transition: opacity 0.3s;
}
